                        <div class="table-responsive">
                            <table  class="table table-bordered table-hover table-striped"  style="width: 100%;">
                                <thead>
                                    <tr>
                                        <th>Phonenumber</th>
                                        <th>Amount</th>
                                        <th>Provider</th>
                                        <th>Token</th>
                                        <th>Card</th>
                                        <th>Class</th>
                                        <th>Quantity</th>
                                        <th>Date</th>
                                        <th>Completed</th>
                                        <th>Refunded</th>
                                        <th>Function</th>
                                        <th>Function</th>
                                        
                                    </tr>
                                </thead>
                                <tbody>
     <tr class="success" style="overflow-y: auto; height:50px;" ng-repeat="order in orderlist |   filterFor3: phone |startFrom: currentPage4 * pageSize4 | limitTo:pageSize4 ">
                                    <!--   <td>{{ $index + 1+ pageSize*currentPage}}</td>-->
    <td>{{order.pnumber}}</td>
    <td>{{order.amount}}</td>
     <td>{{order.provider}}</td>
     <td>{{order.token}}</td>
     <td>{{order.type}}</td>
<td>{{order.class}}</td>
<td>{{order.quantity}}</td>
<td>{{order.date}}</td>
<td>{{order.completed}}</td>
<td>{{order.refunded}}</td>

<td><button class="btn btn-success" ng-click="rerun(order._id)">Rerun</button></td>
<td><button class="btn btn-danger" ng-click="refund(order._id)">Refund</button></td>


                                    </tr>
                                   
                                </tbody>
                                <h2> Page Number</h2>
                                <tr> <button class="btn btn-info"  ng-class="{true:'activeButton'}[activeBtn4 == $index]" ng-repeat="i in getNumberAsArray4(numberOfPages4()) track by $index" ng-click="setCurrentPage4($index)">Pg {{$index + 1}}</button> </tr>
                                
